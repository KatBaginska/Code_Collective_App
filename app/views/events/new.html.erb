<div class="container mt-5">
<div class="new-event">
<h1> <%= link_to "â‡¦", events_path, class: "btn btn-secondary" %> Create event listing</h1>
<%= simple_form_for(@event) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :keywords %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "List", class: "btn btn-primary" %>
<% end %>
</div>
</div>
