<div class="container mt-5">
<h1>Edit event listing</h1>
<%= simple_form_for(@event) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :date%>
  <%= f.input :location %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :keywords %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "List", class: "btn btn-primary" %>
<% end %>
</div>
